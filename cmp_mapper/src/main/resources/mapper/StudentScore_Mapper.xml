<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hfxt.cmp.mapper.IStudentScoreDao">
<!-- 新增的方法写在这里 -->


<!-- 保存非空列,返回受影响的行数 -->
	<insert id="saveSelective" useGeneratedKeys="true" keyProperty="Vo.id">
		Insert Into tb_student_score 
		<trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="Vo.studentId != null">student_id,</if>
            <if test="Vo.testId != null">test_id,</if>
            <if test="Vo.testEvaluate != null">test_evaluate,</if>
            <if test="Vo.studentScore != null">student_score,</if>
            <if test="Vo.desc != null">desc,</if>
           
		</trim>
		<trim prefix="Values (" suffix=")" suffixOverrides=",">
            <if test="Vo.studentId != null">#{Vo.studentId},</if>
            <if test="Vo.testId != null">#{Vo.testId},</if>
            <if test="Vo.testEvaluate != null">#{Vo.testEvaluate},</if>
            <if test="Vo.studentScore != null">#{Vo.studentScore},</if>
            <if test="Vo.desc != null">#{Vo.desc},</if>
		</trim>
	</insert>
    
    <!-- 根据主键删除-->
	<delete id="delete">
		Delete From tb_student_score
		Where id = #{id}
	</delete>
    
    <!-- 删除所有记录,返回受影响的行数 -->
	<delete id="deleteAll">
		Delete From tb_student_score
	</delete>
    
    <!-- 更新非空列,返回受影响的行数 -->
	<update id="updateSelective">
		Update tb_student_score
		<trim prefix="Set" suffixOverrides=",">
            <if test="Vo.studentId != null">student_id = #{Vo.studentId},</if>
            <if test="Vo.testId != null">test_id = #{Vo.testId},</if>
            <if test="Vo.testEvaluate != null">test_evaluate = #{Vo.testEvaluate},</if>
            <if test="Vo.studentScore != null">student_score = #{Vo.studentScore},</if>
            <if test="Vo.desc != null">desc = #{Vo.desc},</if>
		</trim>
		Where tb_student_score.id = #{Vo.id}
	</update>
    
    <resultMap type="StudentScoreVo" id="RM_Default">
        <id column="StudentScore_id" property="id"/>
        <result column="StudentScore_studentId" property="studentId"/>
        <result column="StudentScore_testId" property="testId"/>
        <result column="StudentScore_testEvaluate" property="testEvaluate"/>
        <result column="StudentScore_studentScore" property="studentScore"/>
        <result column="StudentScore_desc" property="desc"/>
	</resultMap>
    
     <!-- ResultMap -->
	<resultMap type="StudentScoreVo" id="RM_getVo_Auto">
		<discriminator javaType="int" column="_chooseRM">
			<case value="0" resultMap="RM_Default" /><!-- Auto -->
		</discriminator>
	</resultMap>
    
    <!-- 根据主键查询一条记录,通过'__chooseRM'控制返回的列 -->
	<select id="getVo_Auto" resultMap="RM_getVo_Auto">
		Select
		<include refid="Sql_SelectColumns" />
		#{chooseRM} As _chooseRM
		From	tb_student_score	As	tb_student_score
		Where
		tb_student_score.id = #{id}
	</select>
    
    <!-- 传入id,判断是否存在该记录 -->
	<select id="isExist_Auto" resultType="boolean">
		Select Count(id) > 0
		From	tb_student_score	As	tb_student_score
		Where
		tb_student_score.id = #{id}
	</select>
    
    <!-- 查询记录数量 -->
	<select id="queryCount_Auto" resultType="Long">
		Select Count(tb_student_score.id)
		From	tb_student_score	As	tb_student_score
        <trim prefix="Where" prefixOverrides="And | Or">
			<include refid="Sql_Where"/>
		</trim>
	</select>
    
    <resultMap type="StudentScoreVo" id="RM_queryVos_Auto">
        <discriminator javaType="int" column="_chooseRM">
			<case value="0" resultMap="RM_Default" /><!-- Auto -->
		</discriminator>
	</resultMap>
    
    <!-- 查询记录集合,通过'__chooseRM'控制返回的列 -->
	<select id="queryVos_Auto" resultMap="RM_queryVos_Auto">
		Select
		<include refid="Sql_SelectColumns"/>
		#{StudentScoreSearch.chooseRM} As _chooseRM
		From	tb_student_score	As	tb_student_score
        <trim prefix="Where" prefixOverrides="And | Or">
			<include refid="Sql_Where"/>
		</trim>
		<include refid="Sql_Order"/>
		Limit #{StudentScoreSearch.offset},#{StudentScoreSearch.limit}
	</select>
    

    <resultMap type="StudentScoreVo" id="RM_getVo_AdminView">
		<!-- <discriminator javaType="int" column="_chooseRM"> -->
        <id column="StudentScore_id" property="id"/>
        <result column="StudentScore_studentId" property="studentId"/>
        <result column="StudentScore_testId" property="testId"/>
        <result column="StudentScore_testEvaluate" property="testEvaluate"/>
        <result column="StudentScore_studentScore" property="studentScore"/>
        <result column="StudentScore_desc" property="desc"/>
		<!-- </discriminator> -->
	</resultMap>
    
    <!-- 根据主键查询一条记录,专供AdminView调用 -->
	<select id="getVo_AdminView" resultMap="RM_getVo_AdminView">
		Select
		<include refid="Sql_SelectColumns" />
		#{chooseRM} As _chooseRM
		From	tb_student_score	As	tb_student_score
		Where
		tb_student_score.id = #{id}
	</select>
    
    <resultMap type="StudentScoreVo" id="RM_getVo_preAdminEdit">
		<!-- <discriminator javaType="int" column="_chooseRM"> -->
        <id column="StudentScore_id" property="id"/>
        <result column="StudentScore_studentId" property="studentId"/>
        <result column="StudentScore_testId" property="testId"/>
        <result column="StudentScore_testEvaluate" property="testEvaluate"/>
        <result column="StudentScore_studentScore" property="studentScore"/>
        <result column="StudentScore_desc" property="desc"/>
		<!-- </discriminator> -->
	</resultMap>
    
    <!-- 根据主键查询一条记录,专供preAdminEdit调用 -->
	<select id="getVo_preAdminEdit" resultMap="RM_getVo_preAdminEdit">
		Select
		<include refid="Sql_SelectColumns" />
		#{chooseRM} As _chooseRM
		From	tb_student_score	As	tb_student_score
		Where
		tb_student_score.id = #{id}
	</select>
    
    <resultMap type="StudentScoreVo" id="RM_getVo_AdminEdit">
		<!-- <discriminator javaType="int" column="_chooseRM"> -->
        <id column="StudentScore_id" property="id"/>
        <result column="StudentScore_studentId" property="studentId"/>
        <result column="StudentScore_testId" property="testId"/>
        <result column="StudentScore_testEvaluate" property="testEvaluate"/>
        <result column="StudentScore_studentScore" property="studentScore"/>
        <result column="StudentScore_desc" property="desc"/>
		<!-- </discriminator> -->
	</resultMap>
    
    <!-- 根据主键查询一条记录,专供AdminEdit调用 -->
	<select id="getVo_AdminEdit" resultMap="RM_getVo_AdminEdit">
		Select
		<include refid="Sql_SelectColumns" />
		#{chooseRM} As _chooseRM
		From	tb_student_score	As	tb_student_score
		Where
		tb_student_score.id = #{id}
	</select>
    
    <resultMap type="StudentScoreVo" id="RM_queryVos_AdminList">
		<!-- <discriminator javaType="int" column="_chooseRM"> -->
        <id column="StudentScore_id" property="id"/>
        <result column="StudentScore_studentId" property="studentId"/>
        <result column="StudentScore_testId" property="testId"/>
        <result column="StudentScore_testEvaluate" property="testEvaluate"/>
        <result column="StudentScore_studentScore" property="studentScore"/>
        <result column="StudentScore_desc" property="desc"/>
		<!-- </discriminator> -->
	</resultMap>
    
    <sql id="Sql_SelectColumns">
        tb_student_score.id As StudentScore_id,
        tb_student_score.student_id As StudentScore_studentId,
        tb_student_score.test_id As StudentScore_testId,
        tb_student_score.test_evaluate As StudentScore_testEvaluate,
        tb_student_score.student_score As StudentScore_studentScore,
        tb_student_score.desc As StudentScore_desc,
	</sql>
    
    <sql id="Sql_Order">
		<trim prefix="Order By ">
    		<choose>
                <when test="StudentScoreSearch.sort != null">
                    StudentScore_${StudentScoreSearch.sort} 
                    <if test="StudentScoreSearch.order != null">
                        ${StudentScoreSearch.order}
                    </if>
                </when>
                <otherwise>
                    StudentScore_id Desc
                </otherwise>
            </choose>
		</trim>
	</sql>
    
    <sql id="Sql_Where">
        <if test="StudentScoreSearch.eqId != null">
			<![CDATA[ And tb_student_score.id = #{StudentScoreSearch.eqId} ]]>
		</if>
		<if test="StudentScoreSearch.neId != null">
			<![CDATA[ And tb_student_score.id != #{StudentScoreSearch.neId} ]]>
		</if>
        <if test="StudentScoreSearch.eqStudentId != null">
			<![CDATA[ And tb_student_score.student_id = #{StudentScoreSearch.eqStudentId} ]]>
		</if>
		<if test="StudentScoreSearch.neStudentId != null">
			<![CDATA[ And tb_student_score.student_id != #{StudentScoreSearch.neStudentId} ]]>
		</if>
        <if test="StudentScoreSearch.eqTestId != null">
			<![CDATA[ And tb_student_score.test_id = #{StudentScoreSearch.eqTestId} ]]>
		</if>
		<if test="StudentScoreSearch.neTestId != null">
			<![CDATA[ And tb_student_score.test_id != #{StudentScoreSearch.neTestId} ]]>
		</if>
        <if test="StudentScoreSearch.eqTestEvaluate != null">
			<![CDATA[ And tb_student_score.test_evaluate = #{StudentScoreSearch.eqTestEvaluate} ]]>
		</if>
		<if test="StudentScoreSearch.lkTestEvaluate != null">
			<![CDATA[ And tb_student_score.test_evaluate Like '%${StudentScoreSearch.lkTestEvaluate}%' ]]>
		</if>
        <if test="StudentScoreSearch.eqStudentScore != null">
			<![CDATA[ And tb_student_score.student_score = #{StudentScoreSearch.eqStudentScore} ]]>
		</if>
		<if test="StudentScoreSearch.neStudentScore != null">
			<![CDATA[ And tb_student_score.student_score != #{StudentScoreSearch.neStudentScore} ]]>
		</if>
        <if test="StudentScoreSearch.eqDesc != null">
			<![CDATA[ And tb_student_score.desc = #{StudentScoreSearch.eqDesc} ]]>
		</if>
		<if test="StudentScoreSearch.lkDesc != null">
			<![CDATA[ And tb_student_score.desc Like '%${StudentScoreSearch.lkDesc}%' ]]>
		</if>
        
    </sql>
</mapper>


